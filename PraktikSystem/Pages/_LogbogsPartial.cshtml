@model List<PraktikSystem.Services.Logbog>

<table class="table">
    <thead>
        <!-- Header rows -->
    </thead>
    <tbody>
        @if (Model != null && Model.Any())
        {
            @foreach (var logbog in Model)
            {
                <tr>
                    <td>@logbog.Id</td>
                    <td>@logbog.Titel</td>
                    <td>@logbog.Beskrivelse</td>
                    <td>@logbog.ArchiveStatus</td>
                    <td>@logbog.Dato.ToString("yyyy-MM-dd HH:mm:ss")</td>
                    <td>
                        <form method="post" asp-page-handler="Archive" asp-route-id="@logbog.Id">
                            <button type="submit">Archive</button>
                        </form>
                    </td>
                </tr>
            }
        }
        else
        {
            <p>No Logbogs Found.</p>
        }
    </tbody>
</table>
